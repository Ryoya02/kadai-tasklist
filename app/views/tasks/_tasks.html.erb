<% tasks.each do |task| %>
        <div>
          <%= link_to task.user.name %> posted at <%= task.created_at %>
        </div>
        <div>
          <p> <%= link_to task.id,task_path(task) %> <%= task.status %>ã€€<%= task.content %></p>
        </div>
        <div>
          <% if current_user == task.user %>
            <%= link_to "Delete", task, method: :delete, data: { confirm: "You sure?" } %>
          <% end %>
        </div>
  <% end %>
  <%= paginate tasks %>



